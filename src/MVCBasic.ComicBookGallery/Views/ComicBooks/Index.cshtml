@model List<MVCBasic.ComicBookGallery.Models.ComicBook>
@{
    ViewBag.Title = "Index";
}
<div class="row">
    @foreach (var m in @Model)
    {
        <div class="col-md-3">
            <h4>@m.DisplayText</h4>
            <img src="/Images/@m.CoverImageFileName"
                 alt="@m.DisplayText"
                 class="img-responsive" />
        </div>
    }
</div>


